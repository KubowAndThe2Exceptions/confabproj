<h1>Advanced Search</h1>

<%= form_with model: @search do |f| %>
    <%= f.label :name %>
    <%= f.text_field :name %>

    <%= f.label :email %>
    <%= f.text_field :email %>

    <%= f.label :address %>
    <%= f.text_field :address %>

    <br><br>

    <%= f.submit "Search" %>
<% end %>

<p><%= link_to "Back", '/users' %></p>