<%= render 'users/menu' %>

<h1>Search User</h1>

<%= form_with model: @search do |f| %>
    <label for="search_input">Search</label>
    <%= f.text_field :search_input %>
    <label for="usertype">Account Type</label>
    <%= f.select :usertype, ["None", "Attendee", "Venue", "Vendor", "Event", "Press"] %>

    <br><br>

    <%= f.submit "Search" %>
<% end %>